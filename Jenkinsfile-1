pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  tools {
    maven 'M3'
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sonar-server') { 
          sh 'mvn clean package sonar:sonar'
        }
      }
    }
  }
}
